<%= simple_form_for(@subscription) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :user_type %>
    <%= f.association :user %>
    <%= f.association :organisation %>
    <%= f.input :expiry_date %>
    <%= f.input :payment %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
