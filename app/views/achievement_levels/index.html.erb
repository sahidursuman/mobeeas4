<p id="notice"><%= notice %></p>

<h1>Listing Achievement Levels</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @achievement_levels.each do |achievement_level| %>
      <tr>
        <td><%= achievement_level.name %></td>
        <td><%= link_to 'Show', achievement_level %></td>
        <td><%= link_to 'Edit', edit_achievement_level_path(achievement_level) %></td>
        <td><%= link_to 'Destroy', achievement_level, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Achievement level', new_achievement_level_path %>
