<div class="row">
  <div class="col s12 m11 offset-m1 l11 offset-l1">
    <%= render 'layouts/sidebar' %>

    <p id="notice"><%= notice %></p>

    <h4>Listing Expression Of Interests</h4>

    <table>
      <thead>
        <tr>
          <th>Sponsor</th>
          <th>Opportunity</th>
          <th>Approved</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @expression_of_interests.each do |expression_of_interest| %>
          <tr>
            <td><%= expression_of_interest.sponsor %></td>
            <td><%= expression_of_interest.opportunity %></td>
            <td><%= expression_of_interest.approved %></td>
            <td><%= link_to 'Show', expression_of_interest %></td>
            <td><%= link_to 'Edit', edit_expression_of_interest_path(expression_of_interest) %></td>
            <td><%= link_to 'Destroy', expression_of_interest, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>

    <br>

  </div>
</div>
