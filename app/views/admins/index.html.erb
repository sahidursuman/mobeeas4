<div class="row">
  <div class="col s12 m11 offset-m1 l11 offset-l1">
		<%= render 'layouts/sidebar' %>
    <p id="notice"><%= notice %></p>

    <h4>Listing Administrators</h4>

    <table>
      <thead>
        <tr>
          <th>First name</th>
          <th>Last name</th>
          <th>Phone</th>
          <th>Address</th>
          <th>Suburb</th>
          <th>State</th>
          <th>Postcode</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @admins.each do |admin| %>
          <tr>
            <td><%= admin.first_name %></td>
            <td><%= admin.last_name %></td>
            <td><%= admin.phone %></td>
            <td><%= admin.address %></td>
            <td><%= admin.suburb %></td>
            <td><%= admin.state %></td>
            <td><%= admin.postcode %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
