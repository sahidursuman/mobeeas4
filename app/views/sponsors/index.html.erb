<h1>Listing Sponsors</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Organisation</th>
      <th>Position</th>
      <th>Phone1</th>
      <th>Phone2</th>
      <th>Address</th>
      <th>Suburb</th>
      <th>State</th>
      <th>Postcode</th>
      <th>Country</th>
      <th>Guid</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sponsors.each do |sponsor| %>
      <tr>
        <td><%= sponsor.first_name %></td>
        <td><%= sponsor.last_name %></td>
        <td><%= sponsor.organisation %></td>
        <td><%= sponsor.position %></td>
        <td><%= sponsor.phone1 %></td>
        <td><%= sponsor.phone2 %></td>
        <td><%= sponsor.address %></td>
        <td><%= sponsor.suburb %></td>
        <td><%= sponsor.state %></td>
        <td><%= sponsor.postcode %></td>
        <td><%= sponsor.country %></td>
        <td><%= sponsor.guid %></td>
        <td><%= sponsor.user %></td>
        <td><%= link_to 'Show', sponsor %></td>
        <td><%= link_to 'Edit', edit_sponsor_path(sponsor) %></td>
        <td><%= link_to 'Destroy', sponsor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sponsor', new_sponsor_path %>
