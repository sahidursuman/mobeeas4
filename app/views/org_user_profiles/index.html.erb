<p id="notice"><%= notice %></p>

<h1>Listing Org User Profiles</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Phone</th>
      <th>Position</th>
      <th>Guid</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @org_user_profiles.each do |org_user_profile| %>
      <tr>
        <td><%= org_user_profile.first_name %></td>
        <td><%= org_user_profile.last_name %></td>
        <td><%= org_user_profile.phone %></td>
        <td><%= org_user_profile.position %></td>
        <td><%= org_user_profile.guid %></td>
        <td><%= org_user_profile.user %></td>
        <td><%= link_to 'Show', org_user_profile %></td>
        <td><%= link_to 'Edit', edit_org_user_profile_path(org_user_profile) %></td>
        <td><%= link_to 'Destroy', org_user_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Org user profile', new_org_user_profile_path %>
